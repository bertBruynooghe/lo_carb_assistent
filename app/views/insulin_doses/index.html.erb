<%= button_to 'New Insulin dose', new_insulin_dose_path, method: :get %>

<table>
  <tr>
    <th> Insulin
    <th> Dose
    <th> Application time
    <th>
    <th>
    <th>
  <% @insulin_doses.each do |insulin_dose| %>
    <tr>
      <td><%= insulin_dose.bolus? ? 'snelwerkend' : 'traagwerkend' %>
      <td><%= insulin_dose.dose %>
      <td data-transform='dateTimeLocal'><%= insulin_dose.application_time.iso8601 %>
      <td><%= link_to 'Show', insulin_dose %>
      <td><%= link_to 'Edit', edit_insulin_dose_path(insulin_dose) %>
      <td><%= link_to 'Destroy', insulin_dose, data: {:confirm => 'Are you sure?'}, :method => :delete %>
  <% end %>
</table>

<% if params[:week] %>
  <%= link_to 'Week nadien', insulin_doses_path(params: { week: week_later_start_date_time(params[:week]) }) %> |
<% end %>
<%= link_to 'Week voordien', insulin_doses_path(params: { week: week_earlier_start_date_time(params[:week]) }) %>

<br>
