<p id="notice"><%= notice %></p>

<h1>Other Meals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meals.each do |meal| %>
      <tr>
        <td data-transform="dateTimeLocal"><%= meal.consumption_time.iso8601 %></td>
        <td><%= link_to 'Show', other_meal_path(meal) %></td>
        <td><%= link_to 'Edit', edit_other_meal_path(meal) %></td>
        <td><%= link_to 'Destroy', meal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Other Meal', new_other_meal_path %>
